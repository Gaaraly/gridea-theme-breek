<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1" />
<title><%= siteTitle %></title>
<meta name="description" content="<%- themeConfig.siteDescription %>" />
<% if ( site.customConfig.jsdelivr == true) { %>
    <link rel="shortcut icon" href="https://cdn.jsdelivr.net/gh/<%= site.customConfig.username %>/<%= site.customConfig.repository %>/favicon.ico">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/<%= site.customConfig.username %>/<%= site.customConfig.repository %>/styles/main.css">
    <% } else{ %>
    <link rel="shortcut icon" href="<%= themeConfig.domain %>/favicon.ico">
    <link rel="stylesheet" href="<%= themeConfig.domain %>/styles/main.css">
<% }; %>


<% if ( site.customConfig.jsdelivr == true) { %>
    <script src="https://cdn.jsdelivr.net/gh/<%= site.customConfig.username %>/<%= site.customConfig.repository %>/media/js/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/gh/<%= site.customConfig.username %>/<%= site.customConfig.repository %>/media/js/masonry.pkgd.min.js"></script>
    <script src="https://cdn.jsdelivr.net/gh/<%= site.customConfig.username %>/<%= site.customConfig.repository %>/media/js/aos.js"></script>
    <script src="https://cdn.jsdelivr.net/gh/<%= site.customConfig.username %>/<%= site.customConfig.repository %>/media/js/pace.min.js"></script>
    <script src="https://cdn.jsdelivr.net/gh/<%= site.customConfig.username %>/<%= site.customConfig.repository %>/media/js/view-image.min.js"></script>
    <script src="https://cdn.jsdelivr.net/gh/<%= site.customConfig.username %>/<%= site.customConfig.repository %>/media/js/jquery.magnific-popup.min.js"></script>
    <script src="https://cdn.jsdelivr.net/gh/<%= site.customConfig.username %>/<%= site.customConfig.repository %>/media/js/functions.js"></script>
    <% } else{ %>
    <script src="<%= themeConfig.domain %>/media/js/jquery.min.js"></script>
    <script src="<%= themeConfig.domain %>/media/js/masonry.pkgd.min.js"></script>
    <script src="<%= themeConfig.domain %>/media/js/aos.js"></script>
    <script src="<%= themeConfig.domain %>/media/js/pace.min.js"></script>
    <script src="<%= themeConfig.domain %>/media/js/view-image.min.js"></script>
    <script src="<%= themeConfig.domain %>/media/js/jquery.magnific-popup.min.js"></script>
    <script src="<%= themeConfig.domain %>/media/js/functions.js"></script>
<% }; %>